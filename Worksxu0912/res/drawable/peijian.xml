<?xml version="1.0" encoding="utf-8"?>
<!-- 是否需要配件的选择器 -->
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_checked="true" android:drawable="@drawable/checked"></item>
    <item android:state_checked="false" android:drawable="@drawable/unchecked"></item>
    
    <item android:state_selected="true" android:drawable="@drawable/checked"></item>
     <item android:state_selected="false" android:drawable="@drawable/unchecked"></item>

</selector>
